
generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Review {
  id        Int      @id @default(autoincrement())
  userId    Int
  code      String
  language  String
  aiFeedback String
  score     Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
