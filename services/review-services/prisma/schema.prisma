
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Review {
  id         Int      @id @default(autoincrement())
  userId     Int
  code       String
  language   String
  aiFeedback String
  score      Int?
  createdAt  DateTime @default(now())
}